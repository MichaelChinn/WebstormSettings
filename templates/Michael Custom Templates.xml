<templateSet group="Michael Custom Templates">
  <template name="fn" value="function $name$($param$) {&#10;    $END$&#10;}" description="function" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="param" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="directivectrl" value="(function() {&#10;    'use strict';&#10;&#10;    angular.module('$mod$')&#10;    .directive('$dir$', $dir$Directive)&#10;    .controller('$dir$Controller', $dir$Controller);&#10;    &#10;    $dir$Directive.$inject = [];&#10;    function $dir$Directive() {&#10;        return {&#10;            restrict: '$r$',&#10;            scope: {},&#10;            controller: '$dir$Controller as vm',&#10;            bindToController: true,&#10;            templateUrl: $url$&#10;        }&#10;    }&#10;    $dir$Controller.$inject = [];&#10;    function $dir$Controller() {&#10;        var vm = this;&#10;        $END$&#10;    }&#10;}) ();" description="directive with controller" toReformat="false" toShortenFQNames="true">
    <variable name="mod" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="dir" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="r" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="url" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="directive" value="(function() {&#10;    'use strict';&#10;&#10;    angular.module('$mod$')&#10;    .directive('$dir$', $dir$Directive);&#10;    &#10;    $dir$Directive.$inject = [];&#10;    function $dir$Directive() {&#10;        return {&#10;            restrict: '$r$',&#10;            templateUrl: '$url$'&#10;            $END$&#10;        }&#10;        &#10;    }&#10;}) ();" description="directive with shared scope" toReformat="false" toShortenFQNames="true">
    <variable name="mod" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="dir" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="r" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="url" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="iffy" value="(function () {&#10;    'use strict';&#10;    $END$&#10;}) ();" description="immediately invoked function" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="controller" value="(function () {&#10;    'use strict';&#10;    &#10;    angular.module('$mod$')&#10;    .controller('$con$Controller', $con$Controller);&#10;    &#10;    $con$Controller.$inject = [];&#10;    function $con$Controller() {&#10;    var vm = this;&#10;    $END$&#10;    }&#10;}) ();" description="controller" toReformat="false" toShortenFQNames="true">
    <variable name="mod" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="con" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name=".state" value=".state('$name$', {&#10;    url: '/$name$',&#10;    parent: '$parent$'$end$&#10;    &#10;})" description="ui router state" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="parent" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="views" value=",&#10;views: {&#10;    '$place$': {&#10;        controller: '$con$Controller as vm',&#10;        templateUrl: '$file$/views/$file$.html'&#10;    }$END$&#10;}" description="ui router views" toReformat="false" toShortenFQNames="true">
    <variable name="place" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="con" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="file" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ctrlfn" value="vm.$fn$ = $fn$;&#10;&#10;function $fn$ ($param$) {&#10;$END$&#10;}" description="function for controller" toReformat="false" toShortenFQNames="true">
    <variable name="fn" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="param" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="script" value="&lt;script src=&quot;$thing$&quot;&gt;&lt;/script&gt;" description="for adding to index" toReformat="false" toShortenFQNames="true">
    <variable name="thing" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="for" value="for(var $v$ in $obj$) {&#10;    $obj$[$v$]&#10;    $END$&#10;}" description="for loop across obj/arr" toReformat="false" toShortenFQNames="true">
    <variable name="v" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="obj" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="if" value="if($cond$) {&#10;    $END$&#10;}" description="if statement" toReformat="false" toShortenFQNames="true">
    <variable name="cond" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="else" value="else {&#10;    $END$&#10;}" description="else clause" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="else if" value="else if($cond$) {&#10;    $END$&#10;}" description="else if clause" toReformat="false" toShortenFQNames="true">
    <variable name="cond" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="var" value="var $t$ = $next$;" description="variable" toReformat="false" toShortenFQNames="true">
    <variable name="t" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="next" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name=".then" value=".then(function($response$) {&#10;    $END$&#10;})" description="then promise" toReformat="false" toShortenFQNames="true">
    <variable name="response" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="r" value="return $thing$;" description="return" toReformat="false" toShortenFQNames="true">
    <variable name="thing" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="nsfn" value=",&#10;$serve$: $serve$&#10;$END$" description="new service function" toReformat="false" toShortenFQNames="true">
    <variable name="serve" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="co" value="console.log($e$);$END$" description="console object" toReformat="false" toShortenFQNames="true">
    <variable name="e" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="$watch" value="$scope.$watch('$thing$' function(newValue, oldValue) {&#10;    $END$&#10;});" description="controller watcher" toReformat="false" toShortenFQNames="true">
    <variable name="thing" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="$on" value="$rootScope.$on('$thing$', function() {&#10;    $END$&#10;});" description="$rootWatcher" toReformat="false" toShortenFQNames="true">
    <variable name="thing" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JS_EXPRESSION" value="true" />
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>